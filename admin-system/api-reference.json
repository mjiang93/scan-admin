{
  "apiInfo": {
    "title": "条码管理系统",
    "version": "1.0.0-SNAPSHOT",
    "baseURL": "http://47.110.53.133:7077",
    "description": "条码管理系统后端API接口文档"
  },
  "commonHeaders": {
    "auth": {
      "type": "string",
      "required": false,
      "description": "认证令牌"
    },
    "sign": {
      "type": "string",
      "required": false,
      "description": "签名"
    }
