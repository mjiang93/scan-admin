  "apis": {
    "printer": {
      "name": "打印机相关接口",
      "endpoints": {
        "list": {
          "path": "/printer/list",
          "method": "GET",
          "summary": "获取打印机列表",
          "params": {
            "pageNum": {
              "type": "string",
              "required": false,
              "default": "1",
              "description": "页码"
            },
            "pageSize": {
              "type": "string",
              "required": false,
              "default": "20",
              "description": "每页大小"
            },
            "status": {
              "type": "string",
              "required": false,
              "default": "ALL",
              "description": "状态筛选：ONLINE/OFFLINE/ALL"
            },
            "department": {
              "type": "string",
              "required": false,
              "description": "部门筛选"
            },
            "keyword": {
              "type": "string",
              "required": false,
              "description": "搜索关键词"
            }
          },
          "response": "返回结果«分页对象«打印机信息»»"
        },
        "add": {
          "path": "/printer/add",
          "method": "POST",
          "summary": "添加打印机",
          "requestBody": "打印机信息对象",
          "response": "返回结果«string»"
        },
        "update": {
          "path": "/printer/update/{printerId}",
          "method": "PUT",
          "summary": "更新打印机信息",
          "params": {
            "printerId": {
              "type": "string",
              "required": true,
              "description": "打印机ID"
            }
          },
          "requestBody": "打印机信息对象",
          "response": "返回结果«boolean»"
        },
        "delete": {
          "path": "/printer/delete/{printerId}",
          "method": "DELETE",
          "summary": "删除打印机",
          "params": {
            "printerId": {
              "type": "string",
              "required": true,
              "description": "打印机ID"
            }
          },
          "response": "返回结果«boolean»"
        },
        "available": {
          "path": "/printer/available",
          "method": "GET",
          "summary": "获取可用打印机列表",
          "params": {
            "department": {
              "type": "string",
              "required": false,
              "description": "部门筛选"
            }
          },
          "response": "返回结果«分页对象«打印机信息»»"
        },
        "departments": {
          "path": "/printer/departments",
          "method": "GET",
          "summary": "获取所有部门列表",
          "response": "返回结果«分页对象«string»»"
        },
        "status": {
          "path": "/printer/status/{printerId}",
          "method": "GET",
          "summary": "检查打印机状态",
          "params": {
            "printerId": {
              "type": "string",
              "required": true,
              "description": "打印机ID"
            }
          },
          "response": "返回结果«打印机信息»"
        },
        "realtimeStatus": {
          "path": "/printer/realtime-status/{printerId}",
          "method": "GET",
          "summary": "获取打印机实时状态",
          "params": {
            "printerId": {
              "type": "string",
              "required": true,
              "description": "打印机ID"
            }
          },
          "response": "返回结果«Map«string,object»»"
        },
        "test": {
          "path": "/printer/test/{printerId}",
          "method": "GET",
          "summary": "测试打印机连接",
          "params": {
            "printerId": {
              "type": "string",
              "required": true,
              "description": "打印机ID"
            }
          },
          "response": "返回结果«boolean»"
        },
        "testPage": {
          "path": "/printer/test-page/{printerId}",
          "method": "GET",
          "summary": "打印测试页",
          "params": {
            "printerId": {
              "type": "string",
              "required": true,
              "description": "打印机ID"
            },
            "operator": {
              "type": "string",
              "required": true,
              "description": "操作人"
            }
          },
          "response": "返回结果«PrintJobRes»"
        },
        "print": {
          "path": "/printer/print",
          "method": "POST",
          "summary": "打印标签（前端控制样式）",
          "requestBody": "PrintLabelReq对象",
          "response": "返回结果«PrintJobRes»"
        },
        "autoPrint": {
          "path": "/printer/print/auto",
          "method": "POST",
          "summary": "自动选择打印机打印",
          "requestBody": "PrintLabelReq对象",
          "response": "返回结果«PrintJobRes»"
        },
        "batchPrint": {
          "path": "/printer/batch-print",
          "method": "POST",
          "summary": "批量打印",
          "requestBody": "PrintLabelReq数组",
          "response": "返回结果«分页对象«PrintJobRes»»"
        },
        "batchPrint48x6": {
          "path": "/printer/print/batch/image/48x6",
          "method": "POST",
          "summary": "批量打印本体码图片（48mm×6mm）",
          "requestBody": "批量图片打印请求",
          "response": "返回结果«分页对象«PrintJobRes»»"
        },
        "batchPrint100x70": {
          "path": "/printer/print/batch/image/100x70",
          "method": "POST",
          "summary": "批量打印内/外包装码图片（100mm×70mm）",
          "requestBody": "批量图片打印请求",
          "response": "返回结果«分页对象«PrintJobRes»»"
        },
        "batchPrintCustom": {
          "path": "/printer/print/batch/image/{size}",
          "method": "POST",
          "summary": "批量打印自定义尺寸图片",
          "params": {
            "size": {
              "type": "string",
              "required": true,
              "description": "图片尺寸（格式：宽度x高度，单位：mm）"
            }
          },
          "requestBody": "批量图片打印请求",
          "response": "返回结果«分页对象«PrintJobRes»»"
        },
        "sendRaw": {
          "path": "/printer/raw/{printerId}",
          "method": "POST",
          "summary": "发送原始ESC/POS指令到打印机",
          "params": {
            "printerId": {
              "type": "string",
              "required": true,
              "description": "打印机ID"
            }
          },
          "requestBody": {
            "commandBase64": {
              "type": "string",
              "required": true,
              "description": "Base64编码的命令"
            }
          },
          "response": "返回结果«boolean»"
        },
        "importExcel": {
          "path": "/printer/import-excel",
          "method": "POST",
          "summary": "导入Excel文件中的打印机",
          "requestBody": "打印机信息数组",
          "response": "返回结果«int»"
        },
        "syncFromExcel": {
          "path": "/printer/sync-from-excel",
          "method": "POST",
          "summary": "从Excel同步打印机信息",
          "response": "返回结果«int»"
        }
      }
    },
    "barcodePrint": {
      "name": "条码打印接口",
      "endpoints": {
        "printBt": {
          "path": "/barcode/print/bt",
          "method": "POST",
          "summary": "打印本体码",
