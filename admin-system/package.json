    "build:check:test": "tsc -b && vite build --mode test",
    "build:check:prod": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "antd": "^5.29.1",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "fast-check": "^4.3.0",
    "jsbarcode": "^3.12.1",
    "jsdom": "^27.0.1",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "vitest": "^3.2.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@fast-check/vitest": "^0.2.3",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.12",
